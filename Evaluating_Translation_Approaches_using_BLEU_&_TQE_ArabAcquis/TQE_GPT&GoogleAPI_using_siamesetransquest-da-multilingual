","\n","        # Add QE score to the dataset\n","        item['GoogleAPI_QE_score'] = googleAPI_QE_score  # Extracting the score\n","\n","        googleAPI_QE_scores.append(googleAPI_QE_score)\n","\n","    # Calculate average QE score\n","    avg_googleAPI_QE_score = sum(googleAPI_QE_scores) / len(googleAPI_QE_scores) if googleAPI_QE_scores else 0\n","\n","    # Add summary to the data\n","    summary = {\n","        \"average_GoogleAPI_QE_score\": avg_googleAPI_QE_score,\n","        \"total_entries\": len(data)\n","    }\n","\n","    return data, summary\n","\n","def main():\n","    input_file = '/content/drive/MyDrive/ColabData/ArabAcquis Dataset/ArabAcquis_with_bleu_scores.json'  # Input JSON file\n","\n","    # Load the JSON data\n","    data = load_json(input_file)\n","\n","    # SAMPLE SIZE: Process only the first 5 entries for testing\n","    sample_data = data[:5]\n","\n","    # Calculate QE scores on the sample\n","    updated_data, summary = calculate_qe_scores(sample_data, model)\n","\n","    # Display the results for the sample\n","    print(\"Sample Results:\")\n","    for item in updated_data:\n","        print(f\"English: {item['english']}\")\n","        print(f\"Google API Translation: {item['googleAPI_translated']}\")\n","        print(f\"GoogleAPI_QE_score: {item['GoogleAPI_QE_score']}\")\n","        print(\"-\" * 80)\n","\n","    # Display summary\n","    print(\"\\nSummary of Sample:\")\n","    print(summary)\n","\n","if __name__ == \"__main__\":\n","    main()\n"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":10729,"status":"ok","timestamp":1727717671468,"user":{"displayName":"Shahad Research","userId":"16465047292842471480"},"user_tz":-180},"id":"qC1qdPAi2vEs","outputId":"6a1b7223-a87e-4fb3-e75c-c3f2d53c8520"},"outputs":[{"name":"stdout","output_type":"stream","text":["\u001b[1;30;43mStreaming output truncated to the last 5000 lines.\u001b[0m\n","Content : {'en