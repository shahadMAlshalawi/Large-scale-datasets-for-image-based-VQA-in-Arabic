\")\n","    return valid_entries\n","\n","def main():\n","    input_file = '/content/drive/MyDrive/ColabData/ArabAcquis Dataset/ArabAcquis_with_bleu_scores.json'  # Input JSON file\n","\n","    # Load the JSON data\n","    data = load_json(input_file)\n","    filtered_data = filter_valid_entries(data)\n","\n","    for index, item in enumerate(filtered_data):\n","        print(f\"Item: {index}\" )\n","        print(f\"Content : {item}\")\n","        print(\"-\" * 80)\n","    #source_target_list = [[item['english'], item['googleAPI_translated']] for item in data]\n","\n","if __name__ == \"__main__\":\n","    main()\n"]},{"cell_type":"markdown","metadata":{"id":"livciYfNVidT"},"source":["\n","\n","---\n","\n"]},{"cell_type":"markdown","metadata":{"id":"ZaFdnZpIgy2p"},"source":["# **Google API QE scores (Handling the dataset in batches)**"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"kghZ3kwdYoay"},"outputs":[],"source":["import json\n","import numpy as np\n","import os\n","from tqdm import tqdm  # Progress bar\n","\n","def load_dataset(input_file):\n","    \"\"\"Loads the dataset from a JSON file.\"\"\"\n","    with open(input_file, 'r', encoding='utf-8') as f:\n","        data = json.load(f)\n","    print(f\"Loaded data with {len(data)} entries.\")\n","    return data\n","\n","def filter_valid_entries(data):\n","    \"\"\"Filter the dataset to only include entries with 'english' and 'googleAPI_translated' fields.\"\"\"\n","    valid_entries = [item for item in data if 'english' in item and 'googleAPI_translated' in item]\n","    print(f\"Filtered dataset to {len(valid_entries)} valid entries.\")\n","    return valid_entries\n","\n","def extract_source_target(data):\n","    \"\"\"Extracts source and target pairs from the data.\"\"\"\n","    source_target_list = [[item['english'], item['googleAPI_translated']] for item in data]\n","    print(f\"Extracted {len(source_target_list)} source-target pairs.\")\n","    return source_target_list\n","\n","def load_existing_predictions(directory):\n","    \"\"\"Loads all existing predictions from saved batch files.\"\"\"\n","    predictions = []\n","    files = sorted([f for f in os.listdir(directory) if f.endswith('.npz')])\n","    for file in files:\n","        data = np.load(os.path.join(directory, file))\n","        predictions.extend(data['predictions'])\n","    return predictions\n","\n","def get_last_saved_batch(directory):\n","    \"\"\"Returns the number of the last saved batch, or 0 if none are found.\"\"\"\n","    files = os.listdir(directory)\n","    batch_numbers = [int(f.split('_')[-1].split('.')[0]) for f in files if f.endswith('.npz')]\n","    return max(batch_numbers) if batch_numbers else 0\n","\n","def predict_in_batches(model, source_target_list, batch_size=100, start_batch=1):\n","    \"\"\"Predicts quality estimates in batches, resuming from a specific batch.\"\"\"\n","    total_batches = len(source_target_list) // batch_size + (1 if len(source_target_list) % batch_size != 0 else 0)\n","\n","    for batch_num in tqdm(range(start_batch, total_batches + 1), total=total_batches, desc=\"Processing Batches\"):\n","        start_idx = (batch_num - 1) * batch_size\n","        batch = source_target_list[start_idx:start_idx + batch_size]\n","\n","        # Initialize a new list to store predictions for the current batch\n","        current_batch_predictions = []\n","\n","        for i, pair in enumerate(batch, start=start_idx):\n","            QE_prediction = model.predict([[pair[0], pair[1]]])\n","            current_batch_predictions.append(QE_prediction)\n","            print(f\"Processed {i + 1} pairs\")\n","\n","        # Save the current batch's predictions\n","        save_predictions(current_batch_predictions, batch_num)\n","\n","def save_predictions(predictions, batch_number):\n","    \"\"\"Saves the predictions of the current batch to a compressed file.\"\"\"\n","    output_file = f'/content/drive/MyDrive/ColabData/ArabAcquis Dataset/googleTransAPI_QE_batches/googleTransAPI_QE_predictions_batch_{batch_number}.npz'\n","    np.savez_compressed(output_file, predictions=predictions)\n","    print(f\"Saved predictions for batch {batch_number} to {output_file}\")\n","\n","def add_predictions_to_data(data, predictions):\n","    \"\"\"Adds the predictions to the original data.\"\"\"\n","    for i, prediction in enumerate(predictions):\n","        data[i]['googleAPI_QE'] = prediction\n","    print(\"Added predictions to data.\")\n","    return data\n","\n","def calculate_average_prediction(predictions):\n","    \"\"\"Calculates the average prediction.\"\"\"\n","    average_prediction = np.mean(predictions)\n","    print(f\"Average googleAPI_QE prediction: {average_prediction}\")\n","    return average_prediction\n","\n","def save_updated_data(data, output_file):\n","    \"\"\"Saves the updated data with predictions to a JSON file.\"\"\"\n","    with open(output_file, 'w', encoding='utf-8') as f:\n","        json.dump(data, f, ensure_ascii=False, indent=4, default=lambda x: float(x) if isinstance(x, np.float32) else x)\n","    print(f\"Data with predictions saved to {output_file}\")\n","\n","def main(input_file, output_file, model):\n","    # Load and process the dataset\n","    data = load_dataset(input_file)\n","\n","    # Filter out invalid entries (without 'english' or 'googleAPI_translated' fields)\n","    filtered_data = filter_valid_entries(data)\n","\n","    source_target_list = extract_source_target(filtered_data)\n","\n","    # Determine where to resume processing\n","    directory = '/content/drive/MyDrive/ColabData/ArabAcquis Dataset/googleTransAPI_QE_batches/'\n","    last_saved_batch = get_last_saved_batch(directory)\n","    print(f\"Resuming from batch {last_saved_batch + 1}\")\n","\n","    # Predict and process in batches starting from the last saved batch\n","    predict_in_batches(model=model, source_target_list=source_target_list, batch_size=100, start_batch=last_saved_batch + 1)\n","\n","    # Load existing predictions from saved batches\n","    predictions = load_existing_predictions(directory)\n","\n","    # Add predictions to filtered_data and save the results\n","    updated_data = add_predictions_to_data(filtered_data, predictions)\n","\n","    # Calculate the average prediction and update the data\n","    average_prediction = calculate_average_prediction(predictions)\n","\n","    # Convert the list to a dictionary\n","    data_dic = {'texts': updated_data}  # Create a dictionary with a 'texts' key\n","    data_dic['average_googleAPI_QE'] = average_prediction\n","\n","    save_updated_data(data_dic, output_file)\n","\n","# Paths to input and output files\n","input_file = '/content/drive/MyDrive/ColabData/ArabAcquis Dataset/ArabAcquis_with_bleu_scores.json'\n","output_file = '/content/drive/MyDrive/ColabData/ArabAcquis Dataset/ArabAcquis_with_googleAPI_QE_scores_and_summary.json'\n","\n","# Assuming 'model' is already defined and loaded\n","main(input_file, output_file, model)\n"]},{"cell_type":"markdown","metadata":{"id":"_gu56U2B0sDt"},"source":["\n","\n","---\n","\n"]},{"cell_type":"markdown","metadata":{"id":"VN2QDfN8CoUA"},"source":["# **GPT QE scores (Handling the dataset in batches)**"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"mDeIrHBlCmyn"},"outputs":[],"source":["import json\n","import numpy as np\n","import os\n","from tqdm import tqdm  # Progress bar\n","\n","def load_dataset(input_file):\n","    \"\"\"Loads the dataset from a JSON file.\"\"\"\n","    with open(input_file, 'r', encoding='utf-8') as f:\n","        data_dic = json.load(f)\n","    data = data_dic['texts']\n","    print(f\"Loaded data with {len(data)} entries.\")\n","    return data\n","\n","def filter_valid_entries(data):\n","    \"\"\"Filter the dataset to only include entries with 'english' and 'GPT_translated' fields.\"\"\"\n","    valid_entries = [item for item in data if 'english' in item and 'GPT_translated' in item]\n","    print(f\"Filtered dataset to {len(valid_entries)} valid entries.\")\n","    return valid_entries\n","\n","def extract_source_target(data):\n","    \"\"\"Extracts source and target pairs from the data.\"\"\"\n","    source_target_list = [[item['english'], item['GPT_translated']] for item in data]\n","    print(f\"Extracted {len(source_target_list)} source-target pairs.\")\n","    return source_target_list\n","\n","def load_existing_predictions(directory):\n","    \"\"\"Loads all existing predictions from saved batch files.\"\"\"\n","    predictions = []\n","    files = sorted([f for f in os.listdir(directory) if f.endswith('.npz')])\n","    for file in files:\n","        data = np.load(os.path.join(directory, file))\n","        predictions.extend(data['predictions'])\n","    return predictions\n","\n","def get_last_saved_batch(directory):\n","    \"\"\"Returns the number of the last saved batch, or 0 if none are found.\"\"\"\n","    files = os.listdir(directory)\n","    batch_numbers = [int(f.split('_')[-1].split('.')[0]) for f in files if f.endswith('.npz')]\n","    return max(batch_numbers) if batch_numbers else 0\n","\n","def predict_in_batches(model, source_target_list, batch_size=100, start_batch=1):\n","    \"\"\"Predicts quality estimates in batches, resuming from a specific batch.\"\"\"\n","    total_batches = len(source_target_list) // batch_size + (1 if len(source_target_list) % batch_size != 0 else 0)\n","\n","    for batch_num in tqdm(range(start_batch, total_batches + 1), total=total_batches, desc=\"Processing Batches\"):\n","        start_idx = (batch_num - 1) * batch_size\n","        batch = source_target_list[start_idx:start_idx + batch_size]\n","\n","        # Initialize a new list to store predictions for the current batch\n","        current_batch_predictions = []\n","\n","        for i, pair in enumerate(batch, start=start_idx):\n","            QE_prediction = model.predict([[pair[0], pair[1]]])\n","            current_batch_predictions.append(QE_prediction)\n","            print(f\"Processed {i + 1} pairs\")\n","\n","        # Save the current batch's predictions\n","        save_predictions(current_batch_predictions, batch_num)\n","\n","def save_predictions(predictions, batch_number):\n","    \"\"\"Saves the predictions of the current batch to a compressed file.\"\"\"\n","    output_file = f'/content/drive/MyDrive/ColabData/ArabAcquis Dataset/GPT_QE_batches/GPT_QE_predictions_batch_{batch_number}.npz'\n","    np.savez_compressed(output_file, predictions=predictions)\n","    print(f\"Saved predictions for batch {batch_number} to {output_file}\")\n","\n","def add_predictions_to_data(data, predictions):\n","    \"\"\"Adds the predictions to the original data.\"\"\"\n","    for i, prediction in enumerate(predictions):\n","        data[i]['GPT_QE'] = prediction\n","    print(\"Added predictions to data.\")\n","    return data\n","\n","def calculate_average_prediction(predictions):\n","    \"\"\"Calculates the average prediction.\"\"\"\n","    average_prediction = np.mean(predictions)\n","    print(f\"Average GPT_QE prediction: {average_prediction}\")\n","    return average_prediction\n","\n","def save_updated_data(data, output_file):\n","    \"\"\"Saves the updated data with predictions to a JSON file.\"\"\"\n","    with open(output_file, 'w', encoding='utf-8') as f:\n","        json.dump(data, f, ensure_ascii=False, indent=4, default=lambda x: float(x) if isinstance(x, np.float32) else x)\n","    print(f\"Data with predictions saved to {output_file}\")\n","\n","def main(input_file, output_file, model):\n","    # Load and process the dataset\n","    data = load_dataset(input_file)\n","\n","    # Filter out invalid entries (without 'english' or 'GPT_translated' fields)\n","    filtered_data = filter_valid_entries(data)\n","\n","    source_target_list = extract_source_target(filtered_data)\n","\n","    # Determine where to resume processing\n","    directory = '/content/drive/MyDrive/ColabData/ArabAcquis Dataset/GPT_QE_batches/'\n","    last_saved_batch = get_last_saved_batch(directory)\n","    print(f\"Resuming from batch {last_saved_batch + 1}\")\n","\n","    # Predict and process in batches starting from the last saved batch\n","    predict_in_batches(model=model, source_target_list=source_target_list, batch_size=100, start_batch=last_saved_batch + 1)\n","\n","    # Load existing predictions from saved batches\n","    predictions = load_existing_predictions(directory)\n","\n","    # Add predictions to filtered_data and save the results\n","    updated_data = add_predictions_to_data(filtered_data, predictions)\n","\n","    # Calculate the average prediction and update the data\n","    average_prediction = calculate_average_prediction(predictions)\n","\n","    # Convert the list to a dictionary\n","    data_dic = {'texts': updated_data}  # Create a dictionary with a 'texts' key\n","    data_dic['average_GPT_QE'] = average_prediction\n","\n","    save_updated_data(data_dic, output_file)\n","\n","# Paths to input and output files\n","input_file = '/content/drive/MyDrive/ColabData/ArabAcquis Dataset/ArabAcquis_with_googleAPI_QE_scores_and_summary.json'\n","output_file = '/content/drive/MyDrive/ColabData/ArabAcquis Dataset/ArabAcquis_with_GPT_QE_scores_and_summary.json'\n","\n","# 'model' is (TransQuest/siamesetransquest-da-multilingual) and already defined and loaded\n","main(input_file, output_file, model)\n"]},{"cell_type":"markdown","metadata":{"id":"LmtG0DsW4H7j"},"source":["\n","\n","---\n","\n"]}],"metadata":{"accelerator":"GPU","colab":{"gpuType":"A100","machine_shape":"hm","provenance":[],"authorship_tag":"ABX9TyOTy90arFluxqT4D70wqeCR"},"kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"},"widgets":{"application/vnd.jupyter.widget-state+json":{"00edba3d0eba4134ad6b280a7e8ba0bc":{"model_module":"@jupyter-widgets/controls","model_module_version":"1.5.0","model_name":"DescriptionStyleModel","state":{"_model_module":"@jupyter-widgets/controls","_model_module_version